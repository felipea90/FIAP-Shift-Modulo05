@{
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-sucess" role="alert">
                <h3>Cliente cadastrado com sucesso !</h3>
                <h4>Id do Cliente: @ViewData["ClienteId"]</h4>
                <h4>Nome do Cliente: @ViewBag.NomeCliente</h4>
            </div>

            <div>
                @{
                    ClienteModel clienteData = ViewData["ClienteModel"] as ClienteModel;
                    ClienteModel clienteBag = ViewBag.ClienteModel;
                    ClienteModel clienteTemp = TempData["ClienteModel"] as ClienteModel;
                }
                <h4>Data: @clienteData.Nome</h4>
                <h4>Bag: @clienteBag.Nome</h4>
                <h4>Temp: @clienteTemp.Nome</h4>
            </div>
        </div>
    </div>
</div>

